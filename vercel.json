{
  "buildCommand": "cd apps/client && pnpm run build",
  "outputDirectory": "apps/client/.next",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "functions": {
    "apps/client/app/api/**/*.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  }
}
